"to shimmy turn left then turn right"

// OPERATOR
trig:
    DO[ act-1 -lex-  shimmy
              -tag-  vimp ]
-----------------
    DO[ act-3 -lex-  turn
              -tag-  vimp
        dir-5 -lex-  left
              -dir-> act-3 ]
    DO[ act-7 -lex-  turn
              -tag-  vimp
        dir-9 -lex-  right
              -dir-> act-7 ]

.................................................

"to shimmy if you are not tired rotate left then rotate right"

// OPERATOR
trig:
    DO[ act-1 -lex-  shimmy
              -tag-  vimp ]
unless:  hq-5 -lex-  tired
              -hq--> obj-3
        obj-3 -lex-  you
-----------------
    DO[ act-7 -lex-  rotate
              -tag-  vimp
        dir-9 -lex-  left
              -dir-> act-7 ]
    DO[ act-11 -lex-  rotate
               -tag-  vimp
        dir-13 -lex-  right
               -dir-> act-11 ]

.................................................

"to shimmy rotate left then rotate right if you are striped"

// OPERATOR
trig:
    DO[  act-1 -lex-  shimmy
               -tag-  vimp
        obj-11 -lex-  you
         hq-13 -lex-  striped
               -hq--> obj-11 ]
-----------------
    DO[ act-3 -lex-  rotate
              -tag-  vimp
        dir-5 -lex-  left
              -dir-> act-3 ]
    DO[ act-7 -lex-  rotate
              -tag-  vimp
        dir-9 -lex-  right
              -dir-> act-7 ]

.................................................

"if you are tired then to cha-cha crash"

// OPERATOR
trig:
    DO[ act-5 -lex-  cha-cha
              -tag-  vimp
         hq-3 -lex-  tired
              -hq--> obj-1
        obj-1 -lex-  you ]
-----------------
    DO[ act-7 -lex-  crash
              -tag-  vimp ]

.................................................

"if you are striped then spin clockwise to cha-cha"

// OPERATOR
trig:
    DO[ act-9 -lex-  cha-cha
              -tag-  vimp
        obj-1 -lex-  you
         hq-3 -lex-  striped
              -hq--> obj-1 ]
-----------------
    DO[ act-5 -lex-  spin
              -tag-  vimp
        dir-7 -lex-  clockwise
              -dir-> act-5 ]

