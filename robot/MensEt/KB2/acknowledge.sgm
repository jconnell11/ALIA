// terms associated with "acknowledge" ops and rules
// ================================================

// singular nouns - mass nouns like "a rice" belong in =[AKO-M] instead

=[AKO]
  rule
  operator


// -----------------------------------------

// imperative verbs

=[ACT]
  understand
  announce
  say
  tell
  acknowledge


// ================================================

// irregular morphologies (npl, acomp, asup, vpres, vprog, vpast)

=[XXX-morph]
  acknowledge * vprog = acknowledging
  acknowledge * vpast = acknowledged
  say * vpast = said
  understand * vpast = understood
  tell * vpast = told


